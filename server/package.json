{
  "name": "flight-booking-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Flight booking application with Amadeus API integration",
  "main": "server/server.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "nodemon server/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "start": "npm run dev:all"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.9",
    "@tailwindcss/postcss": "^4.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "amadeus": "^10.0.0",
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "axios": "^1.7.2",
    "date-fns": "^4.1.0",
    "firebase": "^12.6.0",
    "framer-motion": "^12.23.25",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.263.1",
    "node-fetch": "^2.7.0",
    "react-datepicker": "^8.10.0",
    "swiper": "^12.0.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "vite": "^5.4.1",
    "nodemon": "^3.1.4",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "ext": "js,json",
    "ignore": [
      "server/node_modules"
    ],
    "exec": "node server/server.js"
  }
}
